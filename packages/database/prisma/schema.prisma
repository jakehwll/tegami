// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Feed {
  id      String  @id
  name    String
  url     String
  website String
  entries Entry[]
}

model Category {
  id   String @id
  name String
}

model Entry {
  id     String @id
  name   String
  feedId String
  feed   Feed   @relation(fields: [feedId], references: [id], onDelete: Cascade)
}
